    <script>
        var piclist = "https://n.7k7kimg.cn/uploads/gameimg/202109/ec110.png,https://n.7k7kimg.cn/uploads/gameimg/202108/d2703.png";
        pic = piclist.split(",");

        var hreflist = "https://web.7k7k.com/games/temp_togame.php?target=rxfs_904,https://web.7k7k.com/games/temp_togame.php?target=xajhyy_901";
        href = hreflist.split(",");

        var length = pic.length-1;
        var randomPicIndex = Math.round(Math.random() * length);

        var _id = document.getElementById("wintips");
        _id.style.background="url("+ pic[randomPicIndex] +")no-repeat center/100% 100%";
        document.getElementById("href").href = href[randomPicIndex];

        var dialogNavTip = {
            open: function () {
                $('.wintips').fadeIn();
            },
            event: function () {
                var t = this;
                // 关闭弹窗
                $('.Isee_btnc').on('click', function () {
                    $('.wintips').fadeOut();
                });
                t.open();
            }
        };

        if(!getCookie('7k7k_jrtj')){
            setCookie('7k7k_jrtj',1,86400);
            dialogNavTip.event();
        }
    </script>
